<launch>

<rosparam file="$(find centaur2_description)/launch/controller.yaml" command="load"/>
<node args="Rev22_position_controller Rev23_position_controller joint_state_controller " name="controller_spawner" ns="centaur2" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/centaur2/joint_states"/>
</node>

</launch>